{
    "name": "amazon-ivs-web-broadcast",
    "version": "1.3.1",
    "description": "Amazon IVS Web Broadcast SDK",
    "files": [
        "dist/amazon-ivs-web-broadcast.js",
        "dist/amazon-ivs-web-broadcast-v1.js",
        "dist/index.amazon-ivs-web-broadcast.d.ts",
        "dist/index.amazon-ivs-web-broadcast-v1.d.ts",
        "dist/src/**/*.d.ts"
    ],
    "main": "./dist/amazon-ivs-web-broadcast.js",
    "types": "./dist/index.amazon-ivs-web-broadcast.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.amazon-ivs-web-broadcast.d.ts",
            "default": "./dist/amazon-ivs-web-broadcast.js"
        },
        "./stages-v1": {
            "types": "./dist/index.amazon-ivs-web-broadcast-v1.d.ts",
            "default": "./dist/amazon-ivs-web-broadcast-v1.js"
        }
    },
    "typesVersions": {
        "*": {
            "stages-v1": [
                "./dist/index.amazon-ivs-web-broadcast-v1.d.ts"
            ]
        }
    },
    "homepage": "https://aws.amazon.com/ivs/",
    "bugs": {
        "url": "https://aws.amazon.com/contact-us/"
    },
    "author": {
        "name": "Amazon Web Services",
        "url": "https://aws.amazon.com/"
    },
    "engines": {
        "node": ">=14.15"
    },
    "scripts": {
        "build": "npm run build:js && npm run build:types",
        "build:js": "npx webpack --config ./webpack.config.js",
        "build:prod": "NODE_ENV=production npm run build",
        "build:types": "npx tsc",
        "build:examples": "NODE_ENV=production npx webpack --config ./examples/webpack.config.js",
        "build:ci": "npm run build:prod && npm run build:examples",
        "format": "npx prettier --config .prettierrc --ignore-path .prettierignore --write .",
        "format:check": "npx prettier --config .prettierrc --ignore-path .prettierignore --check .",
        "start": "npx npm-run-all --parallel start:*",
        "start:types": "npx tsc --watch",
        "start:js": "npx webpack --config ./webpack.config.js --watch",
        "lint": "npx eslint ./src --ext .js,.jsx,.ts,.tsx,.json",
        "test": "npx jest --runInBand --collect-coverage --config ./jest.config.js --forceExit",
        "test:e2e": "npx playwright test -c e2e",
        "test:e2e-broadcast": "npx playwright test e2e/specs/broadcast -c e2e",
        "test:e2e-stages": "npx playwright test e2e/specs/stages -c e2e",
        "typedoc": "npx typedoc --options ./typedoc.config.js",
        "examples": "npx webpack serve --config ./examples/webpack.config.js",
        "release": "release-it",
        "bootstrap": "husky install",
        "app": "npm run --prefix test-app/ start",
        "app-lint": "npm run --prefix test-app/ lint",
        "app-check-types": "npm run --prefix test-app/ check-types",
        "app-test": "npm run --prefix test-app/ test",
        "app-ci": "npm run app-lint && npm run app-check-types && npm run app-test",
        "app-install": "npm run --prefix test-app/ app-install",
        "app-setup": "npm run build && npm pack && npm run app-install"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.18.10",
        "@babel/preset-env": "^7.18.10",
        "@babel/preset-typescript": "^7.18.6",
        "@mapbox/node-pre-gyp": "^1.0.10",
        "@playwright/test": "1.30",
        "@release-it/bumper": "^4.0.0",
        "@release-it/keep-a-changelog": "^3.1.0",
        "@types/chai": "^4.2.22",
        "@types/chai-as-promised": "^7.1.4",
        "@types/chai-json-schema": "^1.4.6",
        "@types/jest": "^27.0.2",
        "@types/lodash": "^4.14.181",
        "@types/mocha": "^5.2.7",
        "@types/node": "^16.11.9",
        "@types/offscreencanvas": "^2019.6.4",
        "@types/pixelmatch": "^5.2.4",
        "@types/pngjs": "^6.0.1",
        "@types/react-beautiful-dnd": "^13.1.2",
        "@types/webrtc": "0.0.31",
        "@typescript-eslint/eslint-plugin": "^5.1.0",
        "@typescript-eslint/parser": "^5.1.0",
        "@webpack-cli/serve": "^1.6.0",
        "amazon-ivs-player": "^1.11.0",
        "aws-sdk": "^2.1230.0",
        "babel-loader": "^8.1.0",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^6.4.1",
        "dotenv": "^16.0.3",
        "eslint": "^8.3.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-header": "^3.1.1",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-jsdoc": "^37.6.1",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-prettier": "^4.0.0",
        "fast-check": "^2.18.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "husky": "^7.0.4",
        "jest": "^27.3.1",
        "jest-canvas-mock": "^2.3.1",
        "jest-fetch-mock": "^3.0.3",
        "jest-junit": "^13.0.0",
        "npm-run-all": "^4.1.5",
        "pixelmatch": "^5.3.0",
        "pngjs": "^6.0.0",
        "prettier": "^2.1.2",
        "pretty-quick": "^3.1.3",
        "release-it": "^15.4.2",
        "rimraf": "^3.0.2",
        "serve-handler": "^6.1.3",
        "standardized-audio-context-mock": "^9.0.0",
        "terser-webpack-plugin": "^5.3.7",
        "ts-jest": "^27.0.7",
        "ts-loader": "^8.3.0",
        "typedoc": "^0.23.20",
        "typedoc-plugin-mdn-links": "^2.0.0",
        "typedoc-plugin-missing-exports": "^1.0.0",
        "typescript": "^4.6.2",
        "uuid": "^8.3.2",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.9.2",
        "webpack-merge": "^5.8.0",
        "worker-loader": "^3.0.8",
        "wrtc": "^0.4.7"
    },
    "dependencies": {
        "axios": "^0.27.2",
        "bowser": "^2.11.0",
        "eventemitter3": "^4.0.7",
        "inversify": "^6.0.1",
        "inversify-inject-decorators": "^3.1.0",
        "lodash": "^4.5.0",
        "reflect-metadata": "^0.1.13",
        "webrtc-adapter": "^8.1.1"
    }
}
